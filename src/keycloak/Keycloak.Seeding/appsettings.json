{
  "Serilog": {
    "Using":  [ "Serilog.Sinks.Console" ],
    "MinimumLevel": {
        "Default": "Information",
        "Override": {
          "Microsoft": "Warning",
          "Microsoft.Hosting.Lifetime": "Information"
        }
    },
    "WriteTo": [
      { "Name": "Console" }
    ],
    "Enrich": [
      "FromLogContext",
      "WithMachineName",
      "WithProcessId",
      "WithThreadId",
      "WithCorrelationId"
    ],
    "Properties": {
      "Application": "Org.Eclipse.TractusX.Portal.Backend.Keycloak.Seeding"
    }
  },
  "Keycloak": {
    "central": {
      "ConnectionString": "",
      "User": "",
      "Password": "",
      "AuthRealm": "",
      "UseAuthTrail": false
    }
  },
